<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <TextView
        android:id="@+id/textView16"
        android:layout_width="250dp"
        android:layout_height="115dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="100dp"
        android:text="Change Your Password"

        android:textColor="#676363"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView45"
        android:layout_width="68dp"
        android:layout_height="41dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Back"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView45"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView461"
        android:layout_width="38dp"
        android:layout_height="42dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView45"
        app:layout_constraintVertical_bias="0.331"
        app:srcCompat="@drawable/lock"
        tools:layout_editor_absoluteX="54dp"/>

    <TextView
        android:id="@+id/textView507"
        android:layout_width="197dp"
        android:layout_height="46dp"
        android:fontFamily="@font/Suwargi"
        android:text="Old Password"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="275dp" />

    <ImageView
        android:id="@+id/imageView50"
        android:layout_width="402dp"
        android:layout_height="68dp"
        app:srcCompat="@drawable/line"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="308dp" />

    <ImageView
        android:id="@+id/imageView51"
        android:layout_width="38dp"
        android:layout_height="42dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView45"
        app:layout_constraintVertical_bias="0.496"
        app:srcCompat="@drawable/lock"
        tools:layout_editor_absoluteX="52dp" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="241dp"
        android:layout_height="61dp"
        android:fontFamily="@font/Suwargi"
        android:text="New Password"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="376dp" />

    <ImageView
        android:id="@+id/imageView5011"
        android:layout_width="402dp"
        android:layout_height="68dp"
        app:srcCompat="@drawable/line"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="412dp" />

    <ImageView
        android:id="@+id/imageView610"
        android:layout_width="38dp"
        android:layout_height="42dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView45"
        app:layout_constraintVertical_bias="0.654"
        app:srcCompat="@drawable/lock"
        tools:layout_editor_absoluteX="52dp" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="241dp"
        android:layout_height="61dp"
        android:fontFamily="@font/Suwargi"
        android:text="Confirm New Password"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="470dp" />


</androidx.constraintlayout.widget.ConstraintLayout>